        {
          "name": "Create Cat",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 201\", function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test(\"Response has cat data\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('id');",
                  "    pm.expect(jsonData).to.have.property('name');",
                  "    pm.expect(jsonData).to.have.property('breed');",
                  "    pm.expect(jsonData).to.have.property('years_experience');",
                  "    pm.expect(jsonData).to.have.property('salary');",
                  "});",
                  "",
                  "pm.test(\"Save cat ID for other requests\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.globals.set(\"catId\", jsonData.id);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"name\": \"Agent Whiskers\",\n    \"years_experience\": 5,\n    \"breed\": \"Siamese\",\n    \"salary\": 50000.0\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/cats/",
              "host": ["{{baseUrl}}"],
              "path": ["cats", ""]
            },
            "description": "Create a new spy cat. The breed will be validated against TheCatAPI."
